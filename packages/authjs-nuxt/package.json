{
  "name": "@hebilicious/authjs-nuxt",
  "type": "module",
  "version": "0.0.3",
  "license": "MIT",
  "repository": "Hebilicious/authjs-nuxt",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "*.cjs",
    "*.mjs"
  ],
  "scripts": {
    "build": "pnpm build:module && esno scripts/prepare.ts",
    "build:module": "nuxt-build-module",
    "dev": "nuxi dev"
  },
  "peerDependencies": {
    "nuxt": "3.4.2",
    "vite": "*"
  },
  "dependencies": {
    "@auth/core": "^0.7.1",
    "@nuxt/kit": "3.4.2",
    "@panva/hkdf": "^1.1.1",
    "cookie-es": "^1.0.0",
    "defu": "^6.1.2",
    "immer": "^10.0.1",
    "jose": "^4.14.4",
    "unctx": "^2.3.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.3.1",
    "h3": "^1.6.5",
    "vue": "^3.2.47"
  }
}
